[Unit]
Description=LibyaIntel UNSMIL Playwright upgrader
Wants=network-online.target
After=network-online.target

[Service]
Type=oneshot
User=akram
WorkingDirectory=/home/akram/libyaintel
ExecStart=/home/akram/libyaintel/scripts/run_job.sh runner.jobs.unsmil_playwright_worker
Environment=TZ=UTC
TimeoutStartSec=1800
